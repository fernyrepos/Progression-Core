<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Barbs And Moats</li>
		</mods>
			<match Class="PatchOperationSequence">
				<operations>

					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="RazorWire"]</xpath>
						<value>
							<researchPrerequisites>
								<li>Smithing</li>
							</researchPrerequisites>
						</value>
					</li>

					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="RazorWireGate"]</xpath>
						<value>
							<researchPrerequisites>
								<li>Smithing</li>
							</researchPrerequisites>
						</value>
					</li>

					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="TrenchWalls"]</xpath>
						<value>
							<researchPrerequisites>
								<li>VFET_Construction</li>
							</researchPrerequisites>
						</value>
					</li>

				</operations>
		</match>
	</Operation>
</Patch>